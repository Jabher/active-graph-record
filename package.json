{
  "name": "agregate",
  "version": "0.19.0",
  "description": "A missing piece of simple API for DB",
  "main": "build/index.js",
  "repository": "jabher/agregate",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "test-against-build": "AGREGATE_DIR=build mocha --bail --reporter nyan",
    "build": "babel lib --out-dir build",
    "commit": "git fetch && git pull && jscs --fix lib && [[ `git status --porcelain` ]] && git commit -m \"linting applied \" --no-edit || true && git push -u origin master",
    "preversion": "npm run build && npm run test-against-build && npm run commit",
    "postversion": "npm publish && rm -rf ./build"
  },
  "keywords": [
    "activeRecord",
    "db",
    "agregate",
    "neo4j",
    "cypher"
  ],
  "author": "Vsevolod Rodionov <vsevolod.rodionov@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chai-spies": "^0.7.1",
    "jscs": "^2.7.0",
    "mocha": "^2.3.4"
  },
  "dependencies": {
    "babel-regenerator-runtime": "^6.5.0",
    "core-js": "^2.1.0",
    "cypher-talker": "^0.5.0",
    "debug": "^2.2.0",
    "neo4j": "^2.0.0-RC2",
    "uuid": "^2.0.1"
  }
}
